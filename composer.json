{
    "name": "codezero/laravel-translator",
    "description": "Manage translation files in a Laravel project.",
    "keywords": [
        "laravel",
        "translation",
        "localization",
        "language"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Vermeyen",
            "email": "ivan@codezero.be"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "symfony/var-exporter": "^5.1"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "CodeZero\\Translator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeZero\\Translator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeZero\\Translator\\TranslatorServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
